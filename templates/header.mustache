{{!
    @template theme_moove/header

    Page header template

    Example context (json): {}
}}

{{!
    @template theme_moove/accessibilitymodal

    Blocks drawer template

    Example context (json): {}
}}



<nav class="fixed-top navbar navbar-light navbar-expand moodle-has-zindex">
    {{#hasdrawertoggle}}
        <div data-region="drawer-toggle" class="d-inline-block mr-3 drawer-toggle">
            <button aria-expanded="{{#navdraweropen}}true{{/navdraweropen}}{{^navdraweropen}}false{{/navdraweropen}}" aria-controls="nav-drawer" type="button" class="btn nav-link float-sm-left mr-1" data-action="toggle-drawer" data-side="left" data-preference="drawer-open-nav"><i class="slicon-menu"></i><span class="sr-only">{{#str}}sidepanel, core{{/str}}</span></button>
        </div>
    {{/hasdrawertoggle}}

    <a href="{{{ config.wwwroot }}}" class="navbar-brand {{# output.get_logo }}has-logo{{/ output.get_logo }}
        {{^ output.get_logo }}
            d-none d-sm-inline
        {{/ output.get_logo }}
            ">
        {{# output.get_logo }}
            <span class="logo d-none d-sm-inline">
                <img src="{{output.get_logo}}" alt="{{sitename}}">
            </span>
        {{/ output.get_logo }}
        {{^ output.get_logo }}
            <span class="site-name d-none d-md-inline">{{{ sitename }}}</span>
        {{/ output.get_logo }}
    </a>
    <div class="coursecontrols nav-item d-flex justify-content-center align-items-center">
        {{{ output.teacherdashmenu }}}
        <div class="nav-item nav-link">
            {{{ output.edit_button_fhs }}}
        </div>  
    </div>
    {{> theme_moove/accessibilitybar }}
    <ul class="navbar-nav d-none d-md-flex custom-menus">
        <!-- custom_menu -->
        {{{ output.custom_menu }}}
        <!-- page_heading_menu -->
        {{{ output.page_heading_menu }}}
    </ul>
    <ul class="nav navbar-nav ml-auto mr-3">
        <div class="d-none d-lg-block">

        </div>

        <li class="nav-item accessibility-menu-panel">
            {{> theme_moove/accessibilitybar }}
        </li>
        <div class="popover-regionbar collapsed">
            <div class="popover-regionbar-toggle nav-link icon-no-margin">
                <li class="nav-item accessibility-menu-btn-mod">
                    <span id="themesettings-control-mod">
                        <i title="{{#str}}themesettingstitle, theme_moove{{/str}}"
                            class="fa fa-universal-access">
                        </i>
                    </span>
                </li>
            </div>
        </div>
            

        {{> theme_moove/personalizarDashboard }}
        
        {{{ output.navbar_plugin_output }}}

        {{{ output.user_menu }}}
        
    </ul>
</nav>

